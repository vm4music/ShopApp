                        </div>
                    </div>
                    <div class="_2wWOHm">
                        <div class="_2jj5D3">
                            <div class="_2_z46i"><%= orders.name %></div>
                            <div class="_1J2b7X"><span>(+91) <%= orders.phone %></span><br><%= orders.address %>
                                <!-- <%= JSON.stringify(orders) %> -->
                            </div>
                        </div>

                        <div class="_3lh_J3">

                            <div>

                                <div class="DmOhCx">
                                    <div class="_2sjUET"></div>
                                    <div class="_1EhT9O">
                                        <div class="pcu2bu"></div>
                                        <div class="_3ORfzV">28-05-2021 20:25</div>
                                        <div><span>Parcel has been delivered</span></div>
                                    </div>
                                </div>
                                <div class="DmOhCx">
                                    <div class="_2sjUET"></div>
                                    <div class="_1EhT9O">
                                        <div class="pcu2bu"></div>
                                        <div class="_3ORfzV">28-05-2021 09:21</div>
                                        <div><span>Parcel is out for delivery</span></div>
                                    </div>
                                </div>
                                
                            </div>
                        </div>

                    </div>
                </div>


                <!-- <%= JSON.stringify(orders.items) %> -->
                    
                        <% orders.items.forEach(item=> { %>
                            
                                
                            <div class="item__div">
                                
                                <div class="item__div-image">
                                    <a href="/product/<%= item.item.p_id %>">
                                        <img src="/assets/img/<%= (item.item.img) %>">
                                    </a>
                                </div>
                                <div class="item__div-details">
                                    <div class="item__div-title">
                                        <%= (item.item.name) %>
                                    </div>
                                    <p class="item__div-description">
                                        <!-- <strong>Order#1234</strong> -->
                                        x <%= (item.qty) %>
                                    </p>
                                </div>
                                <div class="item__div-line-price">
                                    <%= (item.item.price) %>
                                </div>
                                
                            </div>
                            <% }) %>
                            <div class="totals">
                                <div class="totals-item">
                                    <label><strong>Total Price: </strong></label>
                                    <div class="totals-value" id="cart-subtotal">
                                        <%= JSON.stringify(orders.cart.totalPrice) %>
                                    </div>
                                </div>
                                <div class="totals-item">
                                    <label><strong>Tax:  </strong></label>
                                    <div class="totals-value" id="cart-subtotal">
                                        <%= JSON.stringify(orders.cart.tax) %>
                                    </div>
                                </div>
                                <div class="totals-item">
                                    <label><strong>Shipping:  </strong></label>
                                    <div class="totals-value" id="cart-subtotal">
                                        <%= JSON.stringify(orders.cart.shipping) %>
                                    </div>
                                </div>
                                <div class="totals-item">
                                    <label><strong>Grand Total:  </strong></label>
                                    <div class="totals-value" id="cart-subtotal">
                                        <%= JSON.stringify(orders.cart.grandTotal) %>
                                    </div>
                                </div>
        
                            </div>
                    </div>
                
                        
            </section>
