<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!--========== BOX ICONS ==========-->
    <link href='https://cdn.jsdelivr.net/npm/boxicons@2.0.5/css/boxicons.min.css' rel='stylesheet'>

    <!--========== CSS ==========-->
    <link rel="stylesheet" href="/assets/css/orderdetail.css">

    <title>My Purchases</title>
</head>

<body>

    <!--========== SCROLL TOP ==========-->
    <a href="#" class="scrolltop" id="scroll-top">
        <i class='bx bx-chevron-up scrolltop__icon'></i>
    </a>

    <!--========== HEADER ==========-->
    <%- include('header') -%>

        <main class="l-main">

            <!--========== ORDERS ==========-->
            <section class="section bd-container">

                <div class="container">
                   
                    <div class="status__row">
                                          <!-- <div class="col-12 col-md-10 hh-grayBox pt45 pb20"> -->
                                              <div class="row justify-content-between">
                                                <% if(orders.status=='Ordered' ) { var created =  orders.created.toString().split(" "); %>
                                                    <div class="order-tracking completed">
                                                        <span class="is-complete"></span>
                                                        <p>Ordered<br><span><%= created[0] %>, <%= created[1] %> <%= created[2] %> </span></p>
                                                    </div>
                                                    <div class="order-tracking">
                                                        <span class="is-complete"></span>
                                                        <p>Shipped<br><span>Tue, June 25</span></p>
                                                    </div>
                                                    <div class="order-tracking">
                                                        <span class="is-complete"></span>
                                                        <p>Delivered<br><span>Fri, June 28</span></p>
                                                    </div>
                                                    <% } %>
                                                    <% if(orders.status=='Shipped' ) { var created =  orders.created.toString().split(" "); %>
                                                        <div class="order-tracking completed">
                                                            <span class="is-complete"></span>
                                                            <p>Ordered<br><span><%= created[0] %>, <%= created[1] %> <%= created[2] %> </span></p>
                                                        </div>
                                                        <div class="order-tracking completed">
                                                            <span class="is-complete"></span>
                                                            <p>Shipped<br><span>Tue, June 25</span></p>
                                                        </div>
                                                        <div class="order-tracking">
                                                            <span class="is-complete"></span>
                                                            <p>Delivered<br><span>Fri, June 28</span></p>
                                                        </div>
                                                        <% } %>
                                                        <% if(orders.status=='Delivered' ) { var created =  orders.created.toString().split(" "); %>
                                                            <div class="order-tracking completed">
                                                                <span class="is-complete"></span>
                                                                <p>Ordered<br><span><%= created[0] %>, <%= created[1] %> <%= created[2] %> </span></p>
                                                            </div>
                                                            <div class="order-tracking completed">
                                                                <span class="is-complete"></span>
                                                                <p>Shipped<br><span>Tue, June 25</span></p>
                                                            </div>
                                                            <div class="order-tracking completed">
                                                                <span class="is-complete"></span>
                                                                <p>Delivered<br><span>Fri, June 28</span></p>
                                                            </div>
                                                            <% } %>
                                                  

                                              </div>
                                          <!-- </div> -->
                                      </div>
                  </div>
                
            <!-- </section> -->

            <!--========== CART ==========-->
            <!-- <section class="section bd-container cart"> -->
